<!-- CPS DTD $Name: HEAD $ $Id: cps.dtd,v 1.1.1.1 2010/03/03 10:44:28 p_prj Exp $ -->
<!ELEMENT sitemap (settings*,(definefragment*|fragment*|storages?|sourcestructures?|resources?|authorization?|security?|snippet?|baseencoding?|sourcepath?|destinationpath?|templatepath?|language*|customtags?|domain*|environment?|include*|buildinterval*|macro*|scheduler?|template+|modules+|sniffer*|category*|node*|page*|shortcut*|images*|fragment*|environmentspecific*)+)>

<!ELEMENT comment (#PCDATA)>

<!ELEMENT environmentspecific ANY>
    <!ATTLIST environmentspecific ignoreon (#PCDATA) #IMPLIED>
    <!ATTLIST environmentspecific useon (#PCDATA) #IMPLIED>
    <!ATTLIST environmentspecific ignoreifpropertyset (#PCDATA) #IMPLIED>
    <!ATTLIST environmentspecific useifpropertyset (#PCDATA) #IMPLIED>

<!-- BASIC CONFIG !deprecated! -->
<!ELEMENT baseencoding (#PCDATA)>
<!ELEMENT sourcepath (#PCDATA)>
<!ELEMENT destinationpath (#PCDATA)>
<!ELEMENT templatepath (#PCDATA)>

<!-- BASIC CONFIG -->
<!ELEMENT settings (#PCDATA|environmentspecific*)>
    <!ATTLIST settings property (editconfpath|templatepath|sourcepath|destinationpath|defaultlang|defaultlangname|wtdsourcedomain|path|url|encoding|userrights) #IMPLIED>
    <!ATTLIST settings propertyname (#PCDATA) #IMPLIED>
    <!ATTLIST settings propertyfile (#PCDATA) #IMPLIED>
    <!ATTLIST settings category (general|attachment) #IMPLIED>

<!ELEMENT environment (production+|test*|development*)>

<!ELEMENT production EMPTY>
    <!ATTLIST production hostname #REQUIRED>

<!ELEMENT test EMPTY>
    <!ATTLIST development hostname #REQUIRED>

<!ELEMENT development EMPTY>
    <!ATTLIST development hostname #IMPLIED>
    <!ATTLIST development os>

<!ELEMENT domain (#PCDATA)>
    <!ATTLIST domain hostname CDATA #IMPLIED>

<!ELEMENT language ( comment?,urlmodifier* )>
    <!ATTLIST language id ID #REQUIRED>
    <!ATTLIST language name CDATA #IMPLIED>

<!-- AUTHORIZATION -->
<!ELEMENT authorization ( users, roles )>

<!ELEMENT users ( user+ )>

<!ELEMENT user EMPTY>
    <!ATTLIST user name CDATA #REQUIRED>
    <!ATTLIST user role CDATA #IMPLIED>
    <!ATTLIST user assignment CDATA #IMPLIED>
    <!ATTLIST user groups IDREF #IMPLIED>

<!ELEMENT roles ( role+ )>

<!ELEMENT role ( right*|rights* )+>
    <!ATTLIST role name ID #REQUIRED>
    <!ATTLIST role rights CDATA #REQUIRED>

<!ELEMENT right EMPTY>
    <!ATTLIST right type (adminusage|publish|editcontent|changedomain|executemacro|storagedelete|storageupload|storagerename) #REQUIRED>

<!--<!ELEMENT rights EMPTY>
    <!ATTLIST rights type CDATA #REQUIRED>-->

<!ELEMENT security ( fileauditlogger, userproperties, users, roles, securityrelations, securitysubsystem )>

<!ELEMENT fileauditlogger EMPTY>
    <!ATTLIST fileauditlogger path CDATA #REQUIRED>

<!ELEMENT userproperties (userproperty+)>

<!ELEMENT userproperty EMPTY>
    <!ATTLIST userproperty id ID #REQUIRED>
    <!ATTLIST userproperty name CDATA #IMPLIED>
    <!ATTLIST userproperty description CDATA #IMPLIED>

<!ELEMENT securityrelations (permission+)>

<!ELEMENT securitysubsystem (classparam+)>
    <!ATTLIST securitysubsystem name CDATA #IMPLIED>
    <!ATTLIST securitysubsystem type CDATA #IMPLIED>

<!ELEMENT permission (classparam+)>
    <!ATTLIST permission group ID #REQUIRED>
    <!ATTLIST permission role IDREF #REQUIRED>
    <!ATTLIST permission module CDATA #IMPLIED>
    <!ATTLIST permission target CDATA #IMPLIED>

<!-- REPETETIVE CONFIGURATION -->
<!ELEMENT definefragment (part+)>
    <!ATTLIST definefragment id ID #REQUIRED>

<!ELEMENT fragment EMPTY>
    <!ATTLIST fragment id IDREF #REQUIRED>

<!-- STRUCTURED SOURCES -->
<!ELEMENT resources ( mysqlconnection*|jndiconnection*|oracleconnection*|sybaseconnection*|sequencer*|environmentspecific*|environmentspecific* )+>

<!ELEMENT sourcestructures ( databasesource*|sqlsource*|filesource*|environmentspecific* )+>

<!ELEMENT mysqlconnection (pool|environmentspecific*)?>
    <!ATTLIST mysqlconnection id ID #REQUIRED>
    <!ATTLIST mysqlconnection user CDATA #REQUIRED>
    <!ATTLIST mysqlconnection password CDATA #REQUIRED>
    <!ATTLIST mysqlconnection database CDATA #REQUIRED>
    <!ATTLIST mysqlconnection hostname CDATA #REQUIRED>
    <!ATTLIST mysqlconnection encoding CDATA #IMPLIED>
    <!ATTLIST mysqlconnection unicode (true|false) #IMPLIED>
    <!ATTLIST mysqlconnection monitoring (true|false) #IMPLIED>
    <!ATTLIST mysqlconnection monitoringwithstackinfo (true|false) #IMPLIED>
    <!ATTLIST mysqlconnection properties CDATA #IMPLIED>
    <!ATTLIST mysqlconnection url CDATA #IMPLIED>

<!ELEMENT oracleconnection (pool|environmentspecific*)?>
    <!ATTLIST oracleconnection id ID #REQUIRED>
    <!ATTLIST oracleconnection user CDATA #REQUIRED>
    <!ATTLIST oracleconnection password CDATA #REQUIRED>
    <!ATTLIST oracleconnection database CDATA #REQUIRED>
    <!ATTLIST oracleconnection hostname CDATA #REQUIRED>
    <!ATTLIST oracleconnection encoding CDATA #IMPLIED>
    <!ATTLIST oracleconnection connectioncaching (true|false) #IMPLIED>
    <!ATTLIST oracleconnection drivertype CDATA #IMPLIED>
    <!ATTLIST oracleconnection monitoring (true|false) #IMPLIED>
    <!ATTLIST oracleconnection monitoringwithstackinfo (true|false) #IMPLIED>
    <!ATTLIST oracleconnection properties CDATA #IMPLIED>
    <!ATTLIST oracleconnection url CDATA #IMPLIED>

<!ELEMENT jndiconnection (pool|environmentspecific*)?>
    <!ATTLIST jndiconnection id ID #REQUIRED>
    <!ATTLIST jndiconnection envname CDATA #REQUIRED>
    <!ATTLIST jndiconnection name CDATA #REQUIRED>
    <!ATTLIST jndiconnection encoding CDATA #IMPLIED>
    <!ATTLIST jndiconnection monitoring (true|false) #IMPLIED>
    <!ATTLIST jndiconnection monitoringwithstackinfo (true|false) #IMPLIED>

<!ELEMENT sybaseconnection (pool|environmentspecific*)?>
    <!ATTLIST sybaseconnection id ID #REQUIRED>
    <!ATTLIST sybaseconnection user CDATA #REQUIRED>
    <!ATTLIST sybaseconnection password CDATA #REQUIRED>
    <!ATTLIST sybaseconnection database CDATA #REQUIRED>
    <!ATTLIST sybaseconnection hostname CDATA #REQUIRED>
    <!ATTLIST sybaseconnection encoding CDATA #IMPLIED>
    <!ATTLIST sybaseconnection monitoring (true|false) #IMPLIED>
    <!ATTLIST sybaseconnection monitoringwithstackinfo (true|false) #IMPLIED>

<!ELEMENT sequencer EMPTY>
    <!ATTLIST sequencer type (file|mysql|oracle) #REQUIRED>
    <!ATTLIST sequencer path CDATA #REQUIRED>
    <!ATTLIST sequencer storage CDATA #REQUIRED>
    <!ATTLIST sequencer name CDATA #REQUIRED>
    <!ATTLIST sequencer connection IDREF #IMPLIED>

<!ELEMENT sqlquery (#PCDATA|param*)>
    <!ATTLIST sqlquery id ID #REQUIRED>
    <!ATTLIST sqlquery template CDATA #REQUIRED>
    <!ATTLIST sqlquery connection IDREF #REQUIRED>

<!ELEMENT pool (parameter*)>
    <!ATTLIST pool type CDATA #IMPLIED>
    <!ATTLIST pool maxactive CDATA #IMPLIED>
    <!ATTLIST pool maxidle CDATA #IMPLIED>
    <!ATTLIST pool maxwait CDATA #IMPLIED>
    <!ATTLIST pool testonborrow (true|false) #IMPLIED>
    <!ATTLIST pool testonreturn (true|false) #IMPLIED>
    <!ATTLIST pool testwhenidle (true|false) #IMPLIED>

<!ELEMENT databasesource ( key?|itemid?|parts?|filerevisionhandler?|sort?|environmentspecific* )+>
    <!ATTLIST databasesource id ID #REQUIRED>
    <!ATTLIST databasesource name CDATA #REQUIRED>
    <!ATTLIST databasesource connection CDATA #REQUIRED>
    <!ATTLIST databasesource table CDATA #REQUIRED>
    <!ATTLIST databasesource ddl CDATA #IMPLIED>
    <!ATTLIST databasesource autocreate (never|onopen|onsave) "never">

<!ELEMENT sqlsource ( listquery|readquery|createquery|updatequery|deletequery|sequencequery? )>
    <!ATTLIST sqlsource id ID #REQUIRED>
    <!ATTLIST sqlsource name CDATA #REQUIRED>
    <!ATTLIST sqlsource guideline CDATA #IMPLIED>
    <!ATTLIST sqlsource connection IDREF #REQUIRED>
    <!ATTLIST sqlsource usecolumnlabel (true|false) #IMPLIED>
    <!ATTLIST sqlsource autocreate (never|onopen|onsave) "never">
    <!ATTLIST sqlsource oncreateitem IDREF #IMPLIED>
    <!ATTLIST sqlsource ondeleteitem IDREF #IMPLIED>
    <!ATTLIST sqlsource onsaveitem IDREF #IMPLIED>
    <!ATTLIST sqlsource onupdateitem IDREF #IMPLIED>
    <!ATTLIST sqlsource beforecreateitem IDREF #IMPLIED>
    <!ATTLIST sqlsource beforesaveitem IDREF #IMPLIED>
    <!ATTLIST sqlsource beforeupdateitem IDREF #IMPLIED>
    <!ATTLIST sqlsource beforedeleteitem IDREF #IMPLIED>
    <!ATTLIST sqlsource aftercreateitem IDREF #IMPLIED>
    <!ATTLIST sqlsource aftersaveitem IDREF #IMPLIED>
    <!ATTLIST sqlsource afterupdateitem IDREF #IMPLIED>
    <!ATTLIST sqlsource afterdeleteitem IDREF #IMPLIED>

<!ELEMENT appendix ( listquery?|readquery?|createquery?|updatequery?|deletequery?|sequencequery? )>

<!ELEMENT listquery ( sqlquery|(param*)+|appendix? )>
    <!ATTLIST listquery itemidcolumn CDATA #REQUIRED>
    <!ATTLIST listquery modifiedcolumn CDATA #IMPLIED>

<!ELEMENT readquery ( sqlquery|(param*)+ )>

<!ELEMENT createquery ( sqlquery|(param*)+|appendix? )>

<!ELEMENT updatequery ( sqlquery|(param*)+|appendix? )>

<!ELEMENT deletequery ( sqlquery|(param*)+|appendix? )>

<!ELEMENT sequencequery ( sqlquery|(param*)+|appendix? )>
    <!ATTLIST listquery timedbefore (true|false) #IMPLIED>
    <!ATTLIST listquery returngeneratedkeys (true|false) #IMPLIED>

<!ELEMENT filesource ( item?|itemid?|parts?|filerevisionhandler?|condition?|sort?|modified?|partfilter?|environmentspecific* )+>
    <!ATTLIST filesource id ID #REQUIRED>
    <!ATTLIST filesource name CDATA #REQUIRED>
    <!ATTLIST filesource charset CDATA #IMPLIED>
    <!ATTLIST filesource sourcepath CDATA #IMPLIED>
    <!ATTLIST filesource guideline CDATA #IMPLIED>
    <!ATTLIST filesource sequencepath CDATA #IMPLIED>
    <!ATTLIST filesource multilanguage (true|false) #IMPLIED>
    <!ATTLIST filesource autocreate (never|onopen|onsave) "never">
    <!ATTLIST filesource storage IDREF #IMPLIED>
    <!ATTLIST filesource oncreateitem IDREF #IMPLIED>
    <!ATTLIST filesource ondeleteitem IDREF #IMPLIED>
    <!ATTLIST filesource onsaveitem IDREF #IMPLIED>
    <!ATTLIST filesource onupdateitem IDREF #IMPLIED>
    <!ATTLIST filesource beforecreateitem IDREF #IMPLIED>
    <!ATTLIST filesource beforesaveitem IDREF #IMPLIED>
    <!ATTLIST filesource beforeupdateitem IDREF #IMPLIED>
    <!ATTLIST filesource beforedeleteitem IDREF #IMPLIED>
    <!ATTLIST filesource aftercreateitem IDREF #IMPLIED>
    <!ATTLIST filesource aftersaveitem IDREF #IMPLIED>
    <!ATTLIST filesource afterupdateitem IDREF #IMPLIED>
    <!ATTLIST filesource afterdeleteitem IDREF #IMPLIED>

<!ELEMENT key ( check* )+>
<!ELEMENT item ( part* )+>
<!ELEMENT itemid ( part?|column? )+>
<!ELEMENT parts ( part*|column*|fragment* )+>

<!ELEMENT modified ( part+ )>
    <!ATTLIST modified datepattern CDATA #REQUIRED>
    <!ATTLIST modified datelocale CDATA #IMPLIED>

<!ELEMENT check EMPTY>
    <!ATTLIST check column CDATA #REQUIRED>
    <!ATTLIST check value CDATA #REQUIRED>

<!ELEMENT column EMPTY>
    <!ATTLIST column name CDATA #REQUIRED>
    <!ATTLIST column partname CDATA #IMPLIED>
    <!ATTLIST column storable (true|false) #IMPLIED>

<!-- REVISION CONTROL -->
<!ELEMENT filerevisionhandler EMPTY>
    <!ATTLIST filerevisionhandler path CDATA #IMPLIED>

<!-- METADATA -->
<!ELEMENT metadata EMPTY>
    <!ATTLIST metadata name CDATA #REQUIRED>
    <!ATTLIST metadata structure CDATA #REQUIRED>
    <!ATTLIST metadata sourcepath CDATA #REQUIRED>
    <!ATTLIST metadata guideline CDATA #IMPLIED>

<!-- SNIPETS -->
<!ELEMENT snippet EMPTY>
    <!ATTLIST snippet id CDATA #REQUIRED>
    <!ATTLIST snippet name CDATA #REQUIRED>
    <!ATTLIST snippet source CDATA #IMPLIED>
    <!ATTLIST snippet structure CDATA #REQUIRED>

<!-- STORAGES -->
<!ELEMENT storages ( filesystemstorage*|environmentspecific* )+>
    <!ATTLIST storages basepath CDATA #IMPLIED>

<!ELEMENT filesystemstorage ( metadata* | exclude* | include* | imagevariant* | gallery* | uploadsettings+ | uploadaction+ |environmentspecific* )*>
    <!ATTLIST filesystemstorage id ID #REQUIRED>
    <!ATTLIST filesystemstorage name CDATA #REQUIRED>
    <!ATTLIST filesystemstorage path CDATA #REQUIRED>
    <!ATTLIST filesystemstorage url CDATA #IMPLIED>
    <!ATTLIST filesystemstorage normalize (true|false) "false">
    <!--<!ATTLIST filesystemstorage secured (true|false) #IMPLIED>-->
    <!--<!ATTLIST filesystemstorage lockDataFilePath CDATA #IMPLIED>-->

<!-- GENERATED IMAGES -->
<!ELEMENT images (image*)>
    <!ATTLIST images basepath CDATA #REQUIRED>
    <!ATTLIST images basestorage IDREF #IMPLIED>

<!ELEMENT image (loadimage*|saveimage*|createimage*|resizeimage*|smartresizeimage*|typetoimage*|canvasresizeimage*|pastetoimage*|cropimage*)>
    <!ATTLIST image id ID #REQUIRED>
    <!ATTLIST image name CDATA #IMPLIED>
    <!ATTLIST image basepath CDATA #REQUIRED>
    <!ATTLIST image url CDATA #REQUIRED>
    <!ATTLIST image previewurl CDATA #IMPLIED>
    <!ATTLIST image alt CDATA #IMPLIED>

<!ELEMENT imagevariant ( loadimage*|saveimage*|createimage*|resizeimage*|smartresizeimage*|typetoimage*|cropimage*|canvasresizeimage* )+>
    <!ATTLIST imagevariant id CDATA #REQUIRED>
    <!ATTLIST imagevariant name CDATA #REQUIRED>
    <!ATTLIST imagevariant format CDATA #REQUIRED>
    <!ATTLIST imagevariant quality CDATA #REQUIRED>
    <!ATTLIST imagevariant path CDATA #REQUIRED>
    <!ATTLIST imagevariant storage CDATA #IMPLIED>

<!ELEMENT resizeimage EMPTY>
    <!ATTLIST resizeimage width CDATA #IMPLIED>
    <!ATTLIST resizeimage height CDATA #IMPLIED>
    <!ATTLIST resizeimage smooth (true|false) #IMPLIED>
    <!ATTLIST resizeimage resizeup (true|false) #IMPLIED>
    <!ATTLIST resizeimage highquality (true|false) #IMPLIED>

<!ELEMENT smartresizeimage EMPTY>
    <!ATTLIST smartresizeimage longer CDATA #IMPLIED>
    <!ATTLIST smartresizeimage shorter CDATA #IMPLIED>
    <!ATTLIST smartresizeimage smooth (true|false) #IMPLIED>
    <!ATTLIST smartresizeimage resizeup (true|false) #IMPLIED>
    <!ATTLIST smartresizeimage highquality (true|false) #IMPLIED>

<!ELEMENT createimage EMPTY>
    <!ATTLIST createimage width CDATA #REQUIRED>
    <!ATTLIST createimage height CDATA #REQUIRED>
    <!ATTLIST createimage background CDATA #IMPLIED>

<!ELEMENT loadimage EMPTY>
    <!ATTLIST loadimage path CDATA #REQUIRED>
    <!ATTLIST loadimage resourcepath CDATA #IMPLIED>
    <!ATTLIST loadimage storage IDREF #IMPLIED>

<!ELEMENT saveimage EMPTY>
    <!ATTLIST saveimage path CDATA #REQUIRED>
    <!ATTLIST saveimage previewpath CDATA #IMPLIED>
    <!ATTLIST saveimage format (jpg|gif|png) #IMPLIED>
    <!ATTLIST saveimage quality CDATA #IMPLIED>
    <!ATTLIST saveimage colordepth CDATA #IMPLIED>
    <!ATTLIST saveimage overwriteage CDATA #IMPLIED>
    <!ATTLIST saveimage transparentcolor CDATA #IMPLIED>
    <!ATTLIST saveimage storage IDREF #IMPLIED>

<!ELEMENT typetoimage EMPTY>
    <!ATTLIST typetoimage text CDATA #REQUIRED>
    <!ATTLIST typetoimage font IDREF #REQUIRED>
    <!ATTLIST typetoimage size CDATA #REQUIRED>
    <!ATTLIST typetoimage style (plain|bold|italic|bolditalic) "plain" #REQUIRED>
    <!ATTLIST typetoimage color CDATA #REQUIRED>
    <!ATTLIST typetoimage x CDATA #REQUIRED>
    <!ATTLIST typetoimage y CDATA #REQUIRED>
    <!ATTLIST typetoimage smooth (true|false) #IMPLIED>
    <!ATTLIST typetoimage alpha CDATA #IMPLIED>
    <!ATTLIST typetoimage align (left|right|center) #IMPLIED>

<!ELEMENT canvasresizeimage EMPTY>
    <!ATTLIST canvasresizeimage width CDATA #REQUIRED>
    <!ATTLIST canvasresizeimage height CDATA #REQUIRED>
    <!ATTLIST canvasresizeimage fillcanvas (true|false) #IMPLIED>
    <!ATTLIST canvasresizeimage smooth (true|false) #IMPLIED>
    <!ATTLIST canvasresizeimage highquality (true|false) #IMPLIED>
    <!ATTLIST canvasresizeimage background CDATA #IMPLIED>

<!ELEMENT pastetoimage EMPTY>
    <!ATTLIST pastetoimage x CDATA #REQUIRED>
    <!ATTLIST pastetoimage y CDATA #REQUIRED>
    <!ATTLIST pastetoimage alpha CDATA #IMPLIED>
    <!ATTLIST pastetoimage path CDATA #REQUIRED>
    <!ATTLIST pastetoimage resourcepath CDATA #IMPLIED>
    <!ATTLIST pastetoimage storage IDREF #IMPLIED>

<!ELEMENT cropimage EMPTY>
    <!ATTLIST cropimage width CDATA #REQUIRED>
    <!ATTLIST cropimage height CDATA #REQUIRED>
    <!ATTLIST cropimage x CDATA #IMPLIED>
    <!ATTLIST cropimage y CDATA #IMPLIED>
    <!ATTLIST cropimage align CDATA #IMPLIED>

<!ELEMENT gallery EMPTY>
    <!ATTLIST gallery id CDATA #IMPLIED>
    <!ATTLIST gallery name CDATA #IMPLIED>
    <!ATTLIST gallery template CDATA #IMPLIED>
    <!ATTLIST gallery sortby (filemodifieddesc) #IMPLIED>

<!ELEMENT uploadsettings EMPTY>
    <!ATTLIST uploadsettings maxuploadsize CDATA #IMPLIED>
    <!ATTLIST uploadsettings replace (true|false) #IMPLIED>
    <!ATTLIST uploadsettings rootupload (true|false) #IMPLIED>
    <!ATTLIST uploadsettings subfoldersupload (true|false) #IMPLIED>
    <!ATTLIST uploadsettings accepted CDATA #IMPLIED>
    <!ATTLIST uploadsettings packetsize CDATA #IMPLIED>

<!ELEMENT uploadaction (classname*|classparam*)>
    <!ATTLIST uploadaction ext CDATA "all" #IMPLIED>
    <!ATTLIST uploadaction method (extractarchive|replacebyvariant|createvariant|delete|updatepublishmetadata|custom|validateimage|exifrotate) #REQUIRED>
    <!ATTLIST uploadaction variant IDREF #IMPLIED>
    <!ATTLIST uploadaction limitHeight CDATA #IMPLIED>
    <!ATTLIST uploadaction limitWidth CDATA #IMPLIED>
    <!ATTLIST uploadaction limitLonger CDATA #IMPLIED>
    <!ATTLIST uploadaction limitShorter CDATA #IMPLIED>
    <!ATTLIST uploadaction userPart CDATA #IMPLIED>
    <!ATTLIST uploadaction sourceStructure IDREF #IMPLIED>
    <!ATTLIST uploadaction fastCheck (true|false) "true">
    <!ATTLIST uploadaction deleteInvalid (true|false) #IMPLIED>

<!-- TEMPLATE CONFIG -->
<!ELEMENT template (comment?|alternate*|area*|structuredarea*|customarea*|filter*|urlmodifier*|after*|before*|replace*)*>
    <!ATTLIST template id ID #REQUIRED>
    <!ATTLIST template extends IDREF #IMPLIED>
    <!ATTLIST template name CDATA #IMPLIED>
    <!ATTLIST template source CDATA #IMPLIED>
    <!ATTLIST template public (true|false) #IMPLIED>
    <!ATTLIST template filterkeywords (true|false) #IMPLIED>
    <!ATTLIST template sourcecharset CDATA #IMPLIED>
    <!ATTLIST template disableutfbom (true|false) #IMPLIED>

<!ELEMENT before (area*|structuredarea*)>
    <!ATTLIST before area CDATA #REQUIRED>

<!ELEMENT after (area*|structuredarea*)>
    <!ATTLIST after area CDATA #REQUIRED>

<!ELEMENT replace (area*|structuredarea*)>
    <!ATTLIST replace area CDATA #REQUIRED>

<!ELEMENT structuredarea (comment?|sort?|group?|filter*|partfilter*)*>
    <!ATTLIST structuredarea id CDATA #IMPLIED>
    <!ATTLIST structuredarea name CDATA #REQUIRED>
    <!ATTLIST structuredarea lang CDATA #IMPLIED>
    <!ATTLIST structuredarea template CDATA #IMPLIED>
    <!ATTLIST structuredarea methodtemplate CDATA #IMPLIED>

<!ELEMENT customarea (comment?|filter*|classname|classparam*)*>
    <!ATTLIST customarea id CDATA #IMPLIED>
    <!ATTLIST customarea name CDATA #REQUIRED>
    <!ATTLIST customarea lang CDATA #IMPLIED>
    <!ATTLIST customarea template CDATA #IMPLIED>
    <!ATTLIST customarea methodtemplate CDATA #IMPLIED>

<!ELEMENT classparam (name,value)>

<!ELEMENT name (#PCDATA)>

<!ELEMENT value (#PCDATA)>

<!ELEMENT sort (sort*)>
    <!ATTLIST sort as (string|int|date) "string">
    <!ATTLIST sort by CDATA #IMPLIED>
    <!ATTLIST sort ascending (true|false) "true">
    <!ATTLIST sort datepattern CDATA #IMPLIED>
    <!ATTLIST sort datelocale CDATA #IMPLIED>
    <!ATTLIST sort locale CDATA #IMPLIED>

<!ELEMENT group EMPTY>
    <!ATTLIST group as (string|int|date) "string">
    <!ATTLIST group by CDATA #IMPLIED>
    <!ATTLIST group ascending (true|false) "true">
    <!ATTLIST group datepattern CDATA #IMPLIED>
    <!ATTLIST group datelocale CDATA #IMPLIED>

<!ELEMENT area (comment?,condition*,partlimit*,start*,stop*,part*,filter*)>
    <!ATTLIST area id CDATA #IMPLIED>
    <!ATTLIST area name CDATA #REQUIRED>
    <!ATTLIST area method (file|node|list|pagedlist|categorylist|navig|navigtree|fulltree|filetree|track2path|pagination|pertinentlink|attachment|keyword|application|objectcontent|include|clear|templateinfo) #IMPLIED>
    <!ATTLIST area level CDATA #IMPLIED>
    <!ATTLIST area param CDATA #IMPLIED>
    <!ATTLIST area lang CDATA #IMPLIED>
    <!ATTLIST area showhidden (true|false) #IMPLIED>
    <!ATTLIST area template CDATA #IMPLIED>
    <!ATTLIST area methodtemplate CDATA #IMPLIED>
    <!ATTLIST area type (page|node|pagelist|pagedlist|categorylist|application|all) #IMPLIED>
    <!ATTLIST area sortby (name|id|date|namedesc|iddesc|datedesc|categorypriority|categoryprioritydesc) #IMPLIED>
    <!ATTLIST area required (true|false) "true">
    <!ATTLIST area guideline CDATA #IMPLIED>

<!ELEMENT part (partlimit*,start*,stop*,partdefault*,filter*)>
    <!ATTLIST part name CDATA #REQUIRED>
    <!ATTLIST part lang CDATA #IMPLIED>
    <!ATTLIST part sniff (name|category|url|onclick|onmouseover|onmouseout|target|priority|targetid|filename|attachment|id|date|type|pertinenturl) #IMPLIED>
    <!ATTLIST part required (true|false) #IMPLIED>
    <!ATTLIST part trim (true|false) "false">

<!ELEMENT start (#PCDATA)>
    <!ATTLIST start include (true|false) #IMPLIED>
    <!ATTLIST start lang IDREF #IMPLIED>

<!ELEMENT stop (#PCDATA)>
    <!ATTLIST stop include (true|false) #IMPLIED>
    <!ATTLIST stop lang IDREF #IMPLIED>

<!ELEMENT partlimit (#PCDATA)>
    <!ATTLIST partlimit include (true|false) #IMPLIED>
    <!ATTLIST partlimit lang CDATA #IMPLIED>

<!ELEMENT partdefault (#PCDATA)>
    <!ATTLIST partdefault lang CDATA #IMPLIED>

<!ELEMENT alternate (alternative+,urlmodifier+)>
    <!ATTLIST alternate id CDATA #REQUIRED>

<!ELEMENT alternative EMPTY>
    <!ATTLIST alternative source CDATA #REQUIRED>
    <!ATTLIST alternative sourcecharset CDATA #IMPLIED>

<!ELEMENT filter (condition*,param?,result?,classname?)>
    <!ATTLIST filter action (removeEmptyLines|removeLineEnds|removeSpaces|removeRedundantSpaces|remove|removeRegExp|recode|processCpsKeywords|processCpsExpressions|processCpsTags|processCpsCustomTags|lowercase|uppercase|formatDate|insertNonBreakSpaces|insertNonBreakSpacesFull|replace|replaceRegExp|ssi|exporter|custom) #IMPLIED>
    <!-- method and process attributes are deprecated by action attribute -->
    <!ATTLIST filter method (custom|remover|decorator|recoder|customtags|formater|internalcps|ssi) #IMPLIED>
    <!ATTLIST filter process (enters|emptylines|lineformating|spaces|nbspaces|custom|lowercase|uppercase|nbspacesfull|date|tags|expressions) #IMPLIED>
    <!ATTLIST filter locale CDATA #IMPLIED>
    <!ATTLIST filter prefix CDATA #IMPLIED>
    <!ATTLIST filter constraint (none|preview) #IMPLIED>

<!ELEMENT partfilter (condition*,param?,result?,classname?)>
    <!ATTLIST partfilter action (removeEmptyLines|removeLineEnds|removeSpaces|removeRedundantSpaces|remove|removeRegExp|recode|processCpsKeywords|processCpsExpressions|processCpsCustomTags|lowercase|uppercase|formatDate|insertNonBreakSpaces|insertNonBreakSpacesFull|replace|replaceRegExp|ssi|virtualpart|custom) #IMPLIED>
    <!ATTLIST partfilter locale CDATA #IMPLIED>
    <!ATTLIST partfilter constraint (none|preview) #IMPLIED>
    <!ATTLIST partfilter part CDATA #IMPLIED>
    <!ATTLIST partfilter as CDATA #IMPLIED>
    <!ATTLIST partfilter template CDATA #IMPLIED>

<!ELEMENT classname (#PCDATA)>
<!ELEMENT param (#PCDATA)>
    <!ATTLIST param trim (true|false) "true">

<!ELEMENT result (#PCDATA)>
    <!ATTLIST result trim (true|false) "true">

<!ELEMENT condition EMPTY>
    <!ATTLIST condition method (part|object|var|ssl|preview|alternate|type|metadata) #REQUIRED>
    <!ATTLIST condition object CDATA #IMPLIED>
    <!ATTLIST condition property CDATA #IMPLIED>
    <!ATTLIST condition type (equals|notequals|contains|contained|notcontains|notcontained|defined|notdefined|datebefore|dateafter) #IMPLIED>
    <!ATTLIST condition value CDATA #IMPLIED>
    <!ATTLIST condition item CDATA #IMPLIED>
    <!ATTLIST condition state (true|false) "true">
    <!ATTLIST condition datepattern CDATA #IMPLIED>
    <!ATTLIST condition datelocale CDATA #IMPLIED>
    <!ATTLIST condition datetomidnight (true|false) "false">
    <!ATTLIST condition skiponpreview (true|false) "false">

<!-- CATEGORIES AND TOOLS CONFIG -->
<!ELEMENT category ( comment?, urlmodifier*, category* )*>
    <!ATTLIST category id ID #REQUIRED>
    <!ATTLIST category name CDATA #REQUIRED>

<!ELEMENT sniffer (comment?,partlimit?,start?,stop?,part*)>
    <!ATTLIST sniffer id ID #REQUIRED>
    <!ATTLIST sniffer name CDATA #IMPLIED>
